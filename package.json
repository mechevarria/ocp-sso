{
  "name": "jboss-client",
  "description": "patternfly-ng client for REST based backend",
  "author": "Mike Echevarria <mechevar@redhat.com>",
  "version": "1.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mechevarria/jboss-client"
  },
  "ng": "node_modules/@angular/cli/bin/ng",
  "scripts": {
    "ng": "$npm_package_ng",
    "local": "$npm_package_ng serve --host 0.0.0.0 --proxy-config proxy.local.json",
    "build": "$npm_package_ng build --prod",
    "buildDev": "$npm_package_ng build --watch",
    "lint": "$npm_package_ng lint",
    "start": "node server.js proxy.ocp.json",
    "startDev": "node server.js proxy.local.json"
  },
  "dependencies": {
    "@angular/animations": "~5.2.11",
    "@angular/cli": "~1.7.4",
    "@angular/common": "~5.2.11",
    "@angular/compiler": "~5.2.11",
    "@angular/compiler-cli": "~5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "~5.2.11",
    "@angular/http": "~5.2.11",
    "@angular/language-service": "~5.2.11",
    "@angular/platform-browser": "~5.2.11",
    "@angular/platform-browser-dynamic": "~5.2.11",
    "@angular/router": "^5.2.11",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.4.2",
    "compression": "^1.7.2",
    "core-js": "^2.4.1",
    "express": "~4.16.0",
    "http-proxy-middleware": "^0.18.0",
    "morgan": "~1.9.0",
    "ngx-bootstrap": "~2.0.5",
    "ngx-breadcrumbs": "0.0.3",
    "npm-sort": "0.0.4",
    "patternfly": "^3.51.2",
    "patternfly-ng": "^3.11.2",
    "rxjs": "~5.5.10",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "zone.js": "^0.8.19"
  }
}
