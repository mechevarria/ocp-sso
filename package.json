{
  "name": "jboss-client",
  "description": "patternfly-ng client for REST based backend",
  "author": "Mike Echevarria <mechevar@redhat.com>",
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mechevarria/jboss-client"
  },
  "ng": "node_modules/@angular/cli/bin/ng",
  "scripts": {
    "ng": "$npm_package_ng",
    "local": "$npm_package_ng serve --host 0.0.0.0 --proxy-config proxy.local.json",
    "build": "$npm_package_ng build --prod",
    "buildDev": "$npm_package_ng build --watch",
    "lint": "$npm_package_ng lint",
    "start": "node server.js proxy.ocp.json",
    "startDev": "node server.js proxy.local.json 4200"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/animations": "~6.0.7",
    "@angular/cli": "~6.0.8",
    "@angular/common": "~6.0.7",
    "@angular/compiler": "~6.0.7",
    "@angular/compiler-cli": "~6.0.7",
    "@angular/core": "~6.0.7",
    "@angular/forms": "~6.0.7",
    "@angular/http": "~6.0.7",
    "@angular/language-service": "~6.0.7",
    "@angular/platform-browser": "~6.0.7",
    "@angular/platform-browser-dynamic": "~6.0.7",
    "@angular/router": "~6.0.7",
    "@exalif/ngx-breadcrumbs": "^6.0.1",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.4.2",
    "compression": "^1.7.2",
    "core-js": "^2.5.7",
    "express": "~4.16.0",
    "http-proxy-middleware": "^0.18.0",
    "morgan": "~1.9.0",
    "ngx-bootstrap": "~3.0.1",
    "npm-sort": "0.0.4",
    "patternfly": "~3.52.1",
    "patternfly-ng": "~4.0.1",
    "rxjs": "~6.2.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.26"
  }
}
